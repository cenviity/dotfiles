        "--wait",
        "--new-window",
        "{{ "{{" }} .Destination {{ "}}" }}",
        "{{ "{{" }} .Source {{ "}}" }}",
        "{{ "{{" }} .Target {{ "}}" }}",
    ]

[data]
    home = {{ .chezmoi.homeDir | quote }}
    brew = {{ output "which" "brew" | trim | quote }}
